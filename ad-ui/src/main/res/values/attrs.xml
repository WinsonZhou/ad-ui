<?xml version="1.0" encoding="utf-8"?>
<resources>

    <declare-styleable name="RateImageView">
        <attr name="image_rate_orientation" format="integer">
            <enum name="horizontal" value="0" />
            <enum name="vertical" value="1" />
        </attr>
        <attr name="image_rate" format="float" />
    </declare-styleable>

    <declare-styleable name="RateLayout">
        <attr name="rate_orientation" format="integer">
            <enum name="horizontal" value="0" />
            <enum name="vertical" value="1" />
        </attr>
        <attr name="rate_layout" format="float" />
    </declare-styleable>

    <declare-styleable name="GridLinearLayout">
        <attr name="column_count" format="integer" />
        <attr name="horizontal_space" format="dimension" />
        <attr name="vertical_space" format="dimension" />
        <attr name="horizontal_divider_mode" format="integer">
            <enum name="middle" value="1" />
            <enum name="normal" value="0" />
        </attr>
        <attr name="vertical_divider_mode" format="integer">
            <enum name="middle" value="1" />
            <enum name="normal" value="0" />
        </attr>
    </declare-styleable>

    <declare-styleable name="PagerIndicator">
        <attr name="IndicatorNormal" format="color" />
        <attr name="IndicatorSelect" format="color" />
    </declare-styleable>

</resources>